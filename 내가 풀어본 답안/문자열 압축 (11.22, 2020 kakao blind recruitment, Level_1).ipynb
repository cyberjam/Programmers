    "# 프로그래머스\n",
    "### kakao blind recruitment\n",
    "### Level_1"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "|날짜|\n",
    "|---------:|\n",
    "|2019.11.22.|"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(s):\n",
    "    answer = len(s)\n",
    "    for i in range(1, int(len(s)/2)+1):\n",
    "        sp = []\n",
    "        for k in range(0,len(s),i):\n",
    "            sp.append(s[k:k+i])\n",
    "        # print(sp)\n",
    "        count = 1\n",
    "        ss=''\n",
    "        for i in range(len(sp)-1):\n",
    "            if sp[i] == sp[i+1]:\n",
    "                count += 1 \n",
    "            else:\n",
    "                if count ==1:\n",
    "                    ss += sp[i]\n",
    "                    count = 1\n",
    "                else:\n",
    "                    ss += str(count)+sp[i]\n",
    "                    count = 1\n",
    "        if count ==1:\n",
    "            ss += sp[-1]\n",
    "        else:\n",
    "            ss += str(count)+sp[i]\n",
    "        answer = min(len(ss), answer)\n",
    "    return answer"
   ]
  },
